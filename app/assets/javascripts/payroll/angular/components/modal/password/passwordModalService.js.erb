(function() {
  'use strict';

  angular.module('somsri.payroll.password_modal.service', [
  ])
  .service('passwordModalService', ['$http', '$log', function($http, $log) {
    return {
      changePassword: function(data) {
        return $http.patch('/settings/update_password', data);
      }
    };
  }]);
})();
