(function() {
  'use strict';
  angular.module('somsri.invoice.groupingReport2.service', [
  ])
  .service('groupingReport2Service', ['$http', function($http) {
    this.getGroupingReport = function(start_date, end_date, options, display_etc) {
      var params = {
        start_date: start_date,
        end_date: end_date
      }
      if(options){
        params["options"] = JSON.stringify(options)
        params["display_etc"] = display_etc
      }else{
        params["display_etc"] = true
      }
      return $http.get('/invoice_grouping', { params: params })
    }

    this.getStudentsWithoutPagination = function(grade_name, year, semester, status) {
      return $http.get('/student_report', { params: { grade_select: grade_name, all: true, year_select: year, semester_select: semester, status: status } })
    }

    this.getGrades = function() {
      return $http.get('/grades')
    }

    this.getYears = function() {
      return $http.get('/invoice_years')
    }

    this.getSemesters = function() {
      return $http.get('/invoice_semesters')
    }
  }]);
})();
