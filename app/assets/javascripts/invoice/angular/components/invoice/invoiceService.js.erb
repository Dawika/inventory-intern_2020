(function() {
  'use strict';

  angular.module('somsri.invoice.invoice.service', [
  ])
  .service('invoiceService', ['$http', function($http) {
    this.newInvoice = function() {
      return $http.get('/invoices/new');
    };

    this.createInvoice = function(data) {
      return $http.post('/invoices', data);
    };

  }]);
})();
