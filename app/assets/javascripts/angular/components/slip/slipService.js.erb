(function() {
  'use strict';

  angular.module('somsri.payroll.service', [
  ])
  .service('slipService', ['$http', function($http) {
    this.employeeSlip = function(id, month, year) {
      return $http.get('/employees/' + id + "/slip", { params: { month: month, year: year } });
    }
  }]);
})();
