(function() {
  'use strict';

  angular.module('somsri.payroll.service', [
  ])
  .service('reportService', ['$http', function($http) {
    this.getPayroll = function(year, month) {
      return $http.get('/reports/' + year + "/" + month);
    }

    this.getMonths = function() {
      return $http.get('/reports/months/');
    }
  }]);
})();
