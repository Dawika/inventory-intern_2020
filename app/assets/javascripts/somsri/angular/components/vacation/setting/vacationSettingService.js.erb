(function() {
  'use strict';
  angular.module('somsri.somsri.vacation.setting.service', [
  ])
  .service('vacationSettingService', ['$http', '$q', function($http, $q) {
    
    this.getVacationSetting = function() {
      return $http.get('/vacation_settings')
    }

    this.saveVacationSetting = function(id, data) {
      return $http.patch('/vacation_settings/' + id, data);
    };

    this.createVacationSetting = function(data) {
      return $http.post('/vacation_settings/', data);
    };

  }]);
})();
