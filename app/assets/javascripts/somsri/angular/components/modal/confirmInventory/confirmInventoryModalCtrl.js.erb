(function() {
  'use strict';
  angular.module('somsri.somsri.confirm_inventory_modal', [
  ])
  .controller('confirmInventoryModalCtrl', ['$scope', '$uibModalInstance', function($scope, $uibModalInstance) {
    this.message = $scope.message;
    this.detail = $scope.detail;
    

    this.ok = function () {
      $uibModalInstance.close(true);
    };

    this.close = function () {
      $uibModalInstance.close(false);
    };
    

  }]);
})();
