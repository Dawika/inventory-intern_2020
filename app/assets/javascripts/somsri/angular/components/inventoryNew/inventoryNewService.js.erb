(function() {
	'use strict';

	angular.module('somsri.somsri.inventoryNew.service', [])
	.service('inventoryNewService', ['$http', 'Upload', function($http, Upload) {
		this.newInventoryNew = function(data) {
			return $http.post('/inventories', data)
		};

		this.getInventoryNew = function() {
			return $http.get('/inventories');
		};

		this.removeInventoryNew = function(id) {
			return $http.delete('/inventories/' + id.id);
		};

		this.updateInventoryNew = function(data, id) {
			return Upload.upload({
							url: '/inventories/' + id,
							method: 'put',
							data: { inventoryNew: data }
						});
		}

	 }]);
})();