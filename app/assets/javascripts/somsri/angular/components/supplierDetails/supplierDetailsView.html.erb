<div class="container" >
  <form ng-submit="supplier.save()">
  <div class="row">
    <div id="inventory">
      <div class="col-xs-12">
        <div class="row mt-50">
          <div class="col-xs-6 mb-20 pl-0">
						<div class="col-md-1 pl-0" style="padding-top: 20px;">
							<i class="fa fa-arrow-left" style="font-size: 30px;" ng-click="supplier.cancel()"></i>
						</div>
						<div class="col-md-11">
							<h1>ชื่อร้านค้า</h1>	
						</div>
          </div>
        </div>
        <div class="row report-content container-fluid panel-board">
          <div class="tab-content">
						<div class="row">
							<div class="col-md-3">
								<div class="form-group">
									<label class="control-label" for="supplier_name">{{ "supplier" | translate }}</label>
									<div class="col-md-12">
										<input class="form-control" autocomplete="off" type="text" id="supplier_name" ng-model="supplier.name" required="true">
									</div>
								</div>
							</div>
							<div class="col-md-5">
								<div class="form-group">
									<label class="control-label" for="address">{{ "address" | translate}} </label>
									<div class="col-md-12">
										<input class="form-control" autocomplete="off" type="text" id="address" ng-model="supplier.address">
									</div>
								</div>
							</div>
							<div class="col-md-4">
								<div class="form-group">
									<label class="control-label" for="phone_number">{{ "phone_number" | translate }}</label>
									<div class="col-md-12">
										<input class="form-control" autocomplete="off" type="text" id="phone_number" ng-model="supplier.phone_number">
									</div>
								</div>
							</div>
						</div>
						<div class="row mt-20">
							<div class="col-md-3">
								<div class="form-group">
									<label class="control-label" for="email">{{ "email" | translate }}</label>
									<div class="col-md-12">
										<input class="form-control" autocomplete="off" type="text" id="email" ng-model="supplier.email" required="true">
									</div>
								</div>
							</div>
						</div>
						<div class="footer-menu row mt-20">
							<div class="pull-right text-right padding-left10">
								<button type="button" class="btn btn-custom btn-gray ng-binding" ng-click="supplier.cancel()">{{ "cancel" | translate }}</button>
							</div>
							<div class="pull-right text-right padding-left10">
								<button type="submit" class="btn btn-custom btn-green ng-binding">{{ supplier.add_button | translate}}</button>
							</div>
						</div>
      		</div>
    		</div>
			</div>
		</div>
  </div>
  </form>
</div>
<div class="row mt-20">
	<div class="container">
		<uib-tabset>
			<div class="col-md-12 panel-board">
				<div class="row">
					<table id="supplier-details-table" >
					</table>
					<div class="row row-centered for-display">
						<div class="col-centered">
							<ul uib-pagination ng-change="supplier.pageSuppliersChanged()" total-items="supplier.suppliersTotalItems" ng-model="supplier.suppliersCurrentPage" max-size="supplier.paginateButtonMaxSize" class="pagination-md" num-pages="numPages" previous-text="‹" next-text="›" boundary-link-numbers='true' rotate='false'></ul>
						</div>
					</div>
				</div>
			</div>
		</uib-tabset>  
		<table  id="tableForExport" data-toolbar="none" data-tableexport-display="always" style="display: none;"><table>
	</div>
</div>
