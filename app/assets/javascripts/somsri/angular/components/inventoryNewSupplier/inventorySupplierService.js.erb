(function() {
	'use strict';

	angular.module('somsri.somsri.inventory.supplier.service', [])
	.service('inventorySupplierService', ['$http', 'Upload', function($http, Upload) {

		this.newSuppliers = function(data) {
			return $http.post('/suppliers', data)
		}

		this.getSuppliers = function(page) {
			return $http.get('/suppliers', {
				params: {
					page: page,
				}
			})
		}

		this.show = function(id) {
   	 return $http.get('/suppliers/' + id)
  	};

  	this.removeSuppliers = function(id) {
			return $http.delete('/suppliers/' + id)
		};

		this.updateSuppliers = function(data, id) {
			return Upload.upload({
							url: '/suppliers/' + id,
							method: 'put',
							data: data 
						});
		};

   }]);
})();