(function() {
  'use strict';

  angular.module('somsri.main.select_member_modal.service', [
  ])
  .service('selectMemberModalService', ['$http', '$log', function($http, $log) {
    return {
      getStudentNotAssignClass : function() {
        return $http.get('/classrooms/student_without_classroom');
      },
      getTeacherNotAssignClass : function(classroomId, currentMemberIds) {
        return $http.get('/classrooms/' + classroomId + '/teacher_without_classroom', {
          params: {
            exclude_ids: JSON.stringify(currentMemberIds)
          }
        });
      }
    };
  }]);
})();
